#!/usr/bin/env bash
# Docker up.
set -euo pipefail
SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
SERVICE_DIR="$(cd "$SCRIPT_DIR/../" && pwd)"
ROOT_DIR="$(cd "$SERVICE_DIR/../../" && pwd)"
source "$ROOT_DIR/lib/common.sh"

cd "$SERVICE_DIR"
print_status "Starting Immich..."
docker compose up -d "$@"
print_success "Immich is running"
